{
  "name": "level51/silverstripe-fluent-autotranslate",
  "version": "1.0.0",
  "time": "2025-10-15",
  "description": "Module for auto translation using Google Translate or OpenAI for Silverstripe with Fluent",
  "keywords": [
    "silverstripe",
    "fluent",
    "translate",
    "autotranslate",
    "google translate",
    "openai"
  ],
  "type": "silverstripe-vendormodule",
  "license": "MIT",
  "authors": [
    {
      "name": "Level51",
      "email": "hallo@lvl51.de"
    }
  ],
  "require": {
    "php": "^8.3",
    "ext-json": "*",
    "ext-intl": "*",
    "silverstripe/framework": "^6.0"
  },
  "extra": {
    "installer-name": "silverstripe-fluent-autotranslate",
    "expose": [
      "client"
    ]
  },
  "minimum-stability": "dev",
  "autoload": {
    "psr-4": {
      "Level51\\Autotranslate\\": "src/"
    }
  }
}
